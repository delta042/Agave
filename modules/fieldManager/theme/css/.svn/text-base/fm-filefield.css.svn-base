.fm-file {
	padding: 4px;
	padding-left: 20px;
	text-decoration: none;
	color: #55E;
	background-position: left center;
	background-repeat: no-repeat;
}
.fm-file:hover {
	color: #33B;
	text-decoration: underline;	
}
.fm-file-editButton {
	font-size: 8px;
	cursor: pointer;
}

/*START link images for file types*/
.fm-file-pdf {
	background-image: url(../images/application-pdf.png);
}
.fm-file-doc, .fm-file-docx {
	background-image: url(../images/x-office-document.png);
}
/*END link images for file types*/